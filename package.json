{
  "name": "generate-changelog-action",
  "description": "This action generates a changelog based on the commits since the last release or a given tag.",
  "version": "1.0.0",
  "author": "scomans",
  "license": "MIT",
  "main": "dist/index.js",
  "scripts": {
    "build": "ncc build ./src/main.ts",
    "start": "npx ncc run ./src/main.ts",
    "lint": "eslint src/**/*.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/scomans/generate-changelog-action.git"
  },
  "keywords": [
    "actions",
    "node",
    "changelog"
  ],
  "dependencies": {
    "@actions/core": "^1.10.0",
    "@actions/github": "^5.1.1",
    "simple-git": "^3.19.1"
  },
  "devDependencies": {
    "@types/node": "^18.17.0",
    "@typescript-eslint/parser": "^6.1.0",
    "@vercel/ncc": "^0.36.1",
    "eslint": "^8.45.0",
    "eslint-plugin-github": "^4.9.2",
    "prettier": "^2.8.3",
    "typescript": "^5.1.6"
  }
}
