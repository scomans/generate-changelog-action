{
  "name": "generate-changelog-action",
  "description": "This action generates a changelog based on the commits since the last release or a given tag.",
  "version": "1.0.0",
  "author": "scomans",
  "license": "MIT",
  "main": "dist/index.js",
  "scripts": {
    "build": "ncc build ./src/main.ts",
    "start": "npx ncc run ./src/main.ts",
    "lint": "eslint src/**/*.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/scomans/generate-changelog-action.git"
  },
  "keywords": [
    "actions",
    "node",
    "changelog"
  ],
  "dependencies": {
    "@actions/core": "^1.10.1",
    "@actions/github": "^6.0.0",
    "simple-git": "^3.25.0"
  },
  "devDependencies": {
    "@types/node": "^20.14.8",
    "@typescript-eslint/parser": "^7.13.1",
    "@vercel/ncc": "^0.38.1",
    "eslint": "^8.57.0",
    "eslint-plugin-github": "^4.10.2",
    "prettier": "^3.3.2",
    "typescript": "^5.5.2"
  }
}
